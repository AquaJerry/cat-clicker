{
  "name": "cat-clicker",
  "version": "0.0.1",
  "description": "Browsering lovely cats",
  "scripts": {
    "build": "webpack",
    "debug": "npm run lint && npm run mocha",
    "doc": "docco *.js src/*.js test/*.js",
    "go": "npm run build && npm run doc && npm run run",
    "lint": "eslint --fix *.js src/*.js test/*.js",
    "mocha": "mocha --require babel-register",
    "run": "node server.js",
    "start": "npm install --production && npm run go",
    "test": "npm run debug && npm run go"
  },
  "devDependencies": {
    "babel-register": "^6.26.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "expect.js": "^0.3.1",
    "mocha": "^4.1.0"
  },
  "dependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.7.0",
    "clean-webpack-plugin": "^0.1.17",
    "docco": "^0.8.0",
    "express": "^4.16.2",
    "html-webpack-plugin": "^2.30.1",
    "knockout": "^3.4.2",
    "uglify-js-plugin": "0.0.6",
    "webpack": "^3.10.0"
  }
}
